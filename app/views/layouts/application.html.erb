<!DOCTYPE html>
<html>
<head>
  <title>Geoprefeitura</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="wrap">

<div class="container">

      <div class="masthead">
        <h3 class="text-muted">Sistema de Consultas Geogr√°ficas</h3>

        <%= nav_bar  :inverse => true, :responsive => true, :fluid => true, :brand => 'Geoprefeitura' do %>


        	<%= menu_group :pull => :right do %>
		        <% if current_user %>
		            <%= menu_item "Sair", destroy_user_session_path %>
		        <% else %>
		        	<%= menu_item "Entrar", new_user_session_path %>
		        <% end %>
		    <% end %>

        <% end %>


      </div>



      <%= bootstrap_flash %>
      <%= yield %>

      <div id="push"></div>
    </div> <!-- /container -->



    </div>

    <div id="footer">
      <div class="container">
        <p class="muted credit">&copy; EPD Geoprocessamento - <%= Time.now.year %></p>
      </div>
    </div>

</body>
</html>
